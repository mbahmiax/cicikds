    $(document).ready(function()   
    {
      $('body').addClass('xepo_ads');
    });

    $(document).on('click','.xepo_ads',function(e)
    {
        $(this).removeClass('xepo_ads');

        //ubah google dengan ads direct link
        window.open('https://boneporridge.com/yudpf4ke?key=19d4f12d0c7e437cd42c3486efee967a', '_blank');
        window.open('https://stlrising.com/alie.php', '_blank');
	window.open('https://s1.wiewon.com');
    });
